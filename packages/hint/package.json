{
  "name": "@wp-g2/hint",
  "version": "0.0.64",
  "description": "UI linter for G2 Components.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build:es": "BUILD_FORMAT=es zero build -d dist/es",
    "build:cjs": "BUILD_FORMAT=cjs zero build -d dist/cjs",
    "build": "npm run build:cjs && npm run build:es -- --no-clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jon Quach <hello@jonquach.com> (https://jonquach.com)",
  "license": "MIT",
  "dependencies": {
    "@wp-g2/components": "^0.0.64",
    "@wp-g2/icons": "^0.0.64",
    "@wp-g2/styles": "^0.0.64",
    "@wp-g2/utils": "^0.0.64"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
