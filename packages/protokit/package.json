{
  "name": "@wp-g2/protokit",
  "version": "0.0.106",
  "description": "Prototyping Toolkit for G2 Components.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "types": "types",
  "scripts": {
    "clean": "rm -rf dist",
    "build:es": "BUILD_FORMAT=es zero build -d dist/es",
    "build:cjs": "BUILD_FORMAT=cjs zero build -d dist/cjs",
    "build": "npm run build:cjs && npm run build:es -- --no-clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jon Quach <hello@jonquach.com> (https://jonquach.com)",
  "license": "MIT",
  "dependencies": {
    "@types/faker": "^5.1.2",
    "@wp-g2/utils": "^0.0.106",
    "faker": "^4.1.0",
    "faker-schema": "^1.0.2",
    "fuse.js": "^6.4.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
