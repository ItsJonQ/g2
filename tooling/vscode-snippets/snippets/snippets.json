		"body": [
			"<Truncate>",
			"  Where the north wind meets the sea, there's a river full of memory. Sleep,",
			"  my darling, safe and sound, for in this river all is found.",
			"</Truncate>"
		],
		"prefix": "g2-truncate"
	},
	"g2-components-view": {
		"body": ["<View>", "  <Text>${1:Into The Unknown}</Text>", "</View>"],
