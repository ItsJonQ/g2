{
	"g2-components-aspectratio": {
		"body": [
			"<AspectRatio ratio={16 / 9}>",
			"  <img",
			"    src=\"https://picsum.photos/seed/picsum/800/800\"",
			"    alt=\"Snowy Mountains\"",
			"    style={{ objectFit: \"cover\" }",
			"  />",
			"</AspectRatio>"
		],
		"prefix": "g2-aspectratio"
	},
	"g2-components-avatar": {
		"body": [
			"<Avatar",
			"  name=\"Elsa Oldenburg\"",
			"  src=\"https://picsum.photos/id/1041/300/300\"",
			"  size=\"medium\"",
			"/>"
		],
		"prefix": "g2-avatar"
	},
	"g2-components-background": {
		"body": [
			"<Background css={[ui.padding(5)]}>",
			"  <Surface css={[ui.padding(5)]}>",
